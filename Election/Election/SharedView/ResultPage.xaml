<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
				  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
				  xmlns:sharedView="clr-namespace:Election.SharedView;assembly=Election"
				  x:Class="Election.SharedView.ResultPage">

    <MasterDetailPage.Master>
      <sharedView:MasterPage x:Name="masterPage" />
    </MasterDetailPage.Master>

    <MasterDetailPage.Detail>
      <NavigationPage>
        
          <x:Arguments>
        <sharedView:Result x:Name="result"/>
          </x:Arguments>
        
      </NavigationPage>
      
    </MasterDetailPage.Detail>

  </MasterDetailPage>


 